

<!DOCTYPE html>
<html lang="en">

<head>
  @await Component.InvokeAsync("HeadViewComponentPartial")
  @await RenderSectionAsync("Styles", required: false)

</head>
<body>
<div class="container-scroller">
  <!-- partial:partials/_navbar.html -->
  @await Component.InvokeAsync("NavbarViewComponentPartial") <!-- partial -->
  <div class="container-fluid page-body-wrapper">
    <!-- partial:partials/_settings-panel.html -->
    <div class="theme-setting-wrapper">
      <div id="settings-trigger"><i class="ti-settings"></i></div>
      <div id="theme-settings" class="settings-panel">
        <i class="settings-close ti-close"></i>
        <p class="settings-heading">SIDEBAR SKINS</p>
        <div class="sidebar-bg-options selected" id="sidebar-light-theme">
          <div class="img-ss rounded-circle bg-light border mr-3"></div>
          Light
        </div>
        <div class="sidebar-bg-options" id="sidebar-dark-theme">
          <div class="img-ss rounded-circle bg-dark border mr-3"></div>
          Dark
        </div>
        <p class="settings-heading mt-2">HEADER SKINS</p>
        <div class="color-tiles mx-0 px-4">
          <div class="tiles success"></div>
          <div class="tiles warning"></div>
          <div class="tiles danger"></div>
          <div class="tiles info"></div>
          <div class="tiles dark"></div>
          <div class="tiles default"></div>
        </div>
      </div>
    </div>

    <!-- partial -->
    @await Component.InvokeAsync("RightSideBarViewComponentPartial")
    <!-- partial:partials/_sidebar.html -->
    @await Component.InvokeAsync("LayoutViewComponentPartial")
    <!-- partial -->
    <div class="main-panel">
      <div class="content-wrapper">

        @RenderBody()
        @RenderSection("Scripts", required: false)

      </div>
      <!-- content-wrapper ends -->
      <!-- partial:partials/_footer.html -->

      <!-- partial -->
    </div>
    <!-- main-panel ends -->
  </div>
  <!-- page-body-wrapper ends -->
</div>
<!-- container-scroller -->

<!-- plugins:js -->
@await Component.InvokeAsync("ScriptViewComponentPartial")<!-- End custom js for this page-->
</body>

</html>